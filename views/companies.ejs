/* تصميم البوكس لكل شركة */
.company {
    background-color: white;
    width: 30%; /* العرض الأساسي */
    margin: 10px; /* المسافة بين العناصر */
    padding: 15px;
    border-radius: 8px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    display: flex;
    align-items: center;
    flex-direction: column; /* ترتيب العناصر داخل البوكس */
    justify-content: center;
    text-align: center;
    transition: transform 0.3s ease, box-shadow 0.3s ease, background-color 0.3s ease; /* الانتقال عند التفاعل */
}

/* تأثير عند المرور بالفأرة */
.company:hover {
    transform: scale(1.05); /* تكبير الحجم قليلاً */
    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.2); /* زيادة الظل عند التمرير */
    background-color: #f0f0f0; /* تغيير لون الخلفية أثناء المرور */
}

/* شعار الشركة */
.company-logo {
    width: 60px;
    height: 60px;
    margin-bottom: 15px;
}

/* نصوص معلومات الشركة */
.company-info {
    font-size: 14px;
    line-height: 1.6;
}

.company-info h3 {
    margin: 5px 0;
    font-size: 16px;
    font-weight: bold;
    color: #333; /* تغيير لون العنوان ليبدو أكثر وضوحاً */
}

.company-info p {
    margin: 3px 0;
    color: #666; /* تغيير لون النص إلى لون رمادي فاتح */
}

/* تحسين العرض للشاشات الصغيرة */
@media (max-width: 768px) {
    .company {
        width: 45%; /* عرض الشركات 45% في الشاشات الصغيرة */
    }
}

@media (max-width: 480px) {
    .company {
        width: 100%; /* عرض الشركات 100% في الشاشات الصغيرة جداً */
    }
}


.profile-img2 {
    width: 125px; /* الحجم الأساسي للصورة */
