    <script>function filterCompanies() {
        const locationFilter = document.getElementById('location-filter').value.toLowerCase().trim();
        const scopeFilter = document.getElementById('scope-filter').value.toLowerCase().trim();
        const companies = document.querySelectorAll('.company');
    
        companies.forEach(company => {
            const companyLocation = company.getAttribute('data-location')?.toLowerCase().trim() || "";
            const companyScopes = company.getAttribute('data-scoop')?.toLowerCase().split(',').map(scope => scope.trim()) || [];
    
            // Match logic
            const locationMatch = !locationFilter || companyLocation === locationFilter;
            const scopeMatch = !scopeFilter || companyScopes.includes(scopeFilter);
    
            // Display company if both filters match
            if (locationMatch && scopeMatch) {
                company.style.display = 'flex'; // Ensure this matches your layout
            } else {
                company.style.display = 'none';
            }
        });
    }
    



        function toggleMenu() {
            var dropdownMenu = document.getElementById("dropdown-menu");
            if (dropdownMenu.style.display === "block") {
                dropdownMenu.style.display = "none";
            } else {
                dropdownMenu.style.display = "block";
            }
        }
        
        document.addEventListener('click', function(event) {
            var dropdownMenu = document.getElementById("dropdown-menu");
            var menuIcon = document.querySelector('.menu-icon');
            if (!menuIcon.contains(event.target) && !dropdownMenu.contains(event.target)) {
                dropdownMenu.style.display = 'none';
            }
        });
    </script>
